        app:layout_constraintTop_toBottomOf="@+id/catHealthPlusMinus" />

    <ImageButton
        android:id="@+id/catShoppingPlusMinus"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="82dp"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/catTransportPlusMinus" />

    <ImageButton
        android:id="@+id/catPersonalCarePlusMinus"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="82dp"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/catShoppingPlusMinus" />

    <ImageButton
        android:id="@+id/catBillsPlusMinus"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="82dp"
        android:layout_marginTop="25dp"
